$(".memory_contribution").html("<% @interest.trustors(current_user).each do |trustor| %><a href='#'><%=raw escape_javascript(content_tag(:div, User.find(trustor).username, :class=>'mind_selection mind_unselected', :data_id => trustor, :title => 'Select the people who trust you ('+User.find(trustor).username+') to add this message to their memory' )) unless trustor == current_user.id %></a><% end %>");